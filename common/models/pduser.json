{
    "name": "Pduser",
    "plural": "pdusers",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "mixins": {
        "TimeStamp": true
    },
    "properties": {
        "name": {
            "type": "string"
        },
        "profileImage": {
            "type": "object"
        },
        "facebookId": {
            "type": "string"
        },
        "googleId": {
            "type": "string"
        },
        "twitterId": {
            "type": "string"
        },
        "facebook": {
            "type": "object"
        },
        "google": {
            "type": "object"
        },
        "points": {
            "type": "number",
            "default": 1000
        },
        "lastContactSync": {
            "type": "date",
            "default": null
        },
        "notificationTokens": {
            "type": "array",
            "default": []
        },
        "authTokens":{
            "type":"array",
            "default":[]
        }
    },
    "validations": [],
    "relations": {
        "contacts": {
            "type": "hasMany",
            "model": "Pdcontact",
            "foreignKey": "userId"
        },
        "_defaultLocation": {
            "type": "embedsOne",
            "model": "Pdlocation",
            "property": "defaultLocation"
        },
        "notifications": {
            "type": "hasMany",
            "model": "Pdnotification",
            "foreignKey": "userId"
        }
    },
    "acls": [],
    "methods": {}
}