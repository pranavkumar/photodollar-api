{
    "name": "Pdrequest",
    "plural": "pdrequests",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "title": {
            "type": "string",
            "required": true
        },
        "expects": {
            "type": "array",
            "default": []
        },
        "forwards": {
            "type": "array",
            "default": []
        },
        "hides": {
            "type": "array",
            "default": []
        },
        "flags": {
            "type": "array",
            "default": []
        },
        "responsesCount":{
            "type":"number",
            "default":0
        }
    },
    "validations": [],
    "relations": {
        "user": {
            "type": "belongsTo",
            "model": "Pduser",
            "foreignKey": "userId"
        },
        "responses": {
            "type": "hasMany",
            "model": "Pdresponse",
            "foreignKey": "requestId"
        },
        "_from": {
            "type": "embedsOne",
            "model": "Pdlocation",
            "property": "from"
        },
        "_to": {
            "type": "embedsOne",
            "model": "Pdlocation",
            "property": "to"
        }
    },
    "acls": [],
    "methods": {},
    "mixins": {
        "TimeStamp" : true
      }
}