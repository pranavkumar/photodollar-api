{
  "name": "UUser",
  "plural": "UUsers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "mobile": {
      "type": "number",
      "required": true
    },
    "mobileCode": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "profileImage":{
      "type":"object"
    },
    "points":{
      "type":"number",
      "default":1000
    },
    "lastContactSync":{
      "type":"date",
      "default":null
    }
  },
  "validations": [],
  "relations": {
    "contacts":{
      "type":"hasMany",
      "model":"UContact",
      "foreignKey":"UUserId"
    }
  },
  "acls": [],
  "methods": {}
}
